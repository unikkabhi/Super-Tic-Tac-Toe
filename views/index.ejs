<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="styles.css">
    <title>Super Tic Tac Toe</title>
</head>
<body>

    <div class="msg-container hide">
      <p id="msg">Winner</p>
      <button id="new-btn">New Game</button>
    </div>
    <main>
      <h1>Super Tic Tac Toe</h1>
      <div class="container">
        <div class="game grid">
          <button class="bigBox hide"></button>
          <button class="box0 box 0_0" name="move" value="00"></button>
          <button class="box0 box 0_1"></button>
          <button class="box0 box 0_2"></button>
          <button class="box0 box 0_3"></button>
          <button class="box0 box 0_4"></button>
          <button class="box0 box 0_5"></button>
          <button class="box0 box 0_6"></button>
          <button class="box0 box 0_7"></button>
          <button class="box0 box 0_8"></button>
        </div>
        <div class="game grid">
           <button class="bigBox hide"></button>
            <button class="box1 box 1_0"></button>
            <button class="box1 box 1_1"></button>
            <button class="box1 box 1_2"></button>
            <button class="box1 box 1_3"></button>
            <button class="box1 box 1_4"></button>
            <button class="box1 box 1_5"></button>
            <button class="box1 box 1_6"></button>
            <button class="box1 box 1_7"></button>
            <button class="box1 box 1_8"></button>
        </div>

        <div class="game grid">
           <button class="bigBox hide"></button>
            <button class="box2 box 2_0"></button>
            <button class="box2 box 2_1"></button>
            <button class="box2 box 2_2"></button>
            <button class="box2 box 2_3"></button>
            <button class="box2 box 2_4"></button>
            <button class="box2 box 2_5"></button>
            <button class="box2 box 2_6"></button>
            <button class="box2 box 2_7"></button>
            <button class="box2 box 2_8"></button>
        </div>

      </div>
      <hr>
      <div class="container">
        <div class="game grid">
          <button class="bigBox hide"></button>
          <button class="box3 box 3_0"></button>
          <button class="box3 box 3_1"></button>
          <button class="box3 box 3_2"></button>
          <button class="box3 box 3_3"></button>
          <button class="box3 box 3_4"></button>
          <button class="box3 box 3_5"></button>
          <button class="box3 box 3_6"></button>
          <button class="box3 box 3_7"></button>
          <button class="box3 box 3_8"></button>
        </div>
        <div class="game grid">
          <button class="bigBox hide"></button>
            <button class="box4 box 4_0"></button>
            <button class="box4 box 4_1"></button>
            <button class="box4 box 4_2"></button>
            <button class="box4 box 4_3"></button>
            <button class="box4 box 4_4"></button>
            <button class="box4 box 4_5"></button>
            <button class="box4 box 4_6"></button>
            <button class="box4 box 4_7"></button>
            <button class="box4 box 4_8"></button>
        </div>

        <div class="game grid">
          <button class="bigBox hide"></button>
            <button class="box5 box 5_0"></button>
            <button class="box5 box 5_1"></button>
            <button class="box5 box 5_2"></button>
            <button class="box5 box 5_3"></button>
            <button class="box5 box 5_4"></button>
            <button class="box5 box 5_5"></button>
            <button class="box5 box 5_6"></button>
            <button class="box5 box 5_7"></button>
            <button class="box5 box 5_8"></button>
        </div>

      </div>
      <hr>
      <div class="container">
        <div class="game grid">
          <button class="bigBox hide"></button>
          <button class="box6 box 6_0"></button>
          <button class="box6 box 6_1"></button>
          <button class="box6 box 6_2"></button>
          <button class="box6 box 6_3"></button>
          <button class="box6 box 6_4"></button>
          <button class="box6 box 6_5"></button>
          <button class="box6 box 6_6"></button>
          <button class="box6 box 6_7"></button>
          <button class="box6 box 6_8"></button>
        </div>
        <div class="game grid">
          <button class="bigBox hide"></button>
            <button class="box7 box 7_0"></button>
            <button class="box7 box 7_1"></button>
            <button class="box7 box 7_2"></button>
            <button class="box7 box 7_3"></button>
            <button class="box7 box 7_4"></button>
            <button class="box7 box 7_5"></button>
            <button class="box7 box 7_6"></button>
            <button class="box7 box 7_7"></button>
            <button class="box7 box 7_8"></button>
        </div>

        <div class="game grid">
          <button class="bigBox hide"></button>
            <button class="box8 box 8_0"></button>
            <button class="box8 box 8_1"></button>
            <button class="box8 box 8_2"></button>
            <button class="box8 box 8_3"></button>
            <button class="box8 box 8_4"></button>
            <button class="box8 box 8_5"></button>
            <button class="box8 box 8_6"></button>
            <button class="box8 box 8_7"></button>
            <button class="box8 box 8_8"></button>
        </div>

      </div>
      <button id="reset-btn">Reset Game</button>
    </main>

    <script src="app.js"></script>
</body>
</html>